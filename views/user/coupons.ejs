<%- include('header.ejs') %>
    <div class="page-wrapper">
        <main class="main">
            <div class="page-header text-center" style="background-image: url('assets/images/page-header-bg.jpg')">
                <div class="container">
                    <h1 class="page-title"><span>ELECTRO MANIA</span></h1>
                </div><!-- End .container -->
            </div><!-- End .page-header -->
            <nav aria-label="breadcrumb" class="breadcrumb-nav">
                <div class="container">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/">Home</a></li>
                        <li class="breadcrumb-item"><a href="/shop">Shop</a></li>
                        <li class="breadcrumb-item active" aria-current="page">wishlist</li>
                    </ol>
                </div><!-- End .container -->
            </nav><!-- End .breadcrumb-nav -->

            <div class="page-content">
                <div class="cart">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12">
                                <% if(data.length==0){ %>
                                    <h6>
                                        No Coupons available .
                                    </h6>
                                    <% } else { %>
                                        <table class="table table-cart table-mobile">
                                            <thead>
                                                <tr>
                                                    <th>Coupon code</th>
                                                    <th></th>
                                                    <th>Expiry date</th>
                                                    <th>Offer Amount</th>
                                                    <th>Crieteria Amount</th>
                                                    


                                                </tr>
                                            </thead>




                                            <tbody>

                                                <% if(data) { %>
                                                    <% data.map((item)=>{ %>

                                                    <% if(item.used==="Used"){ %>
                                                        <tr style="opacity: 0.7;">
                                                            <td class="product-col">
                                                                <div class="product">
                                                                    
    
                                                                    <h3 class="" style="border: 1px dotted #000;   
                                                                    padding: 10px;">
                                                                       
                                                                            <%= item.couponcode %>
                                                                        
                                                                    </h3>
                                                                    
    
    
                                                                </div>
    
                                                            </td>
                                                            <td> <button class="btn btn-primary"  style="background-color: rgb(149, 0, 0); border-radius: 6px;">Used</button></td>
                                                            <td class="price-col"><%= item.expirydate.toLocaleDateString() %></td>
                                                            <td class="price-col">₹ <%= item.discountamount %></td>
    
                                                            <td class="price-col">
                                                               ₹ <%=  item.criteriaamount %>
                                                            </td>
    
    
                                                            
                                                        </tr>
                                                    <% }else{ %>
                                                    <tr>
                                                        <td class="product-col">
                                                            <div class="product">
                                                                

                                                                <h3 class="" style="border: 1px dotted #000;   
                                                                padding: 10px;">
                                                                   
                                                                        <%= item.couponcode %>
                                                                    
                                                                </h3>
                                                                


                                                            </div>

                                                        </td>
                                                        <td></td>
                                                        <td class="price-col"><%= item.expirydate.toLocaleDateString() %></td>
                                                        <td class="price-col">₹ <%= item.discountamount %></td>

                                                        <td class="price-col">
                                                           ₹ <%=  item.criteriaamount %>
                                                        </td>


                                                        
                                                    </tr>
                                                    <% } %>

                                                    <% }) %>
                                                    <% } %>
                                                    

                                            </tbody>

                                        </table>
                                        <% } %>
                                            <!-- End .table table-wishlist -->

                                            <div class="cart-bottom">


                                                <a href="/updatewish" class="btn btn-outline-dark-2"><span>UPDATE
                                                        CART</span><i class="icon-refresh"></i></a>
                                            </div><!-- End .cart-bottom -->
                            </div><!-- End .col-lg-9 -->

                        </div><!-- End .row -->

                    </div><!-- End .container -->
                </div><!-- End .cart -->
            </div><!-- End .page-content -->
        </main><!-- End .main -->

        <footer class="footer footer" style="color: black;">
            <div class="footer-middle">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-6 col-lg-3">
                            <div class="widget widget-about">
                                <img src="/images/Screenshot 2023-09-12 110622.png" class="footer-logo" alt="Footer Logo" width="105" height="25">
                                <p style="color: black;">ELECTRO MANIA the perfect choice for digital world  </p>
    
                                <div class="social-icons">
                                    <a href="https://www.instagram.com/mohd_sharhan_/#" class="social-icon" title="Facebook" target="_blank"><i class="icon-facebook-f"></i></a>
                                    <a href="https://www.instagram.com/mohd_sharhan_/#" class="social-icon" title="Twitter" target="_blank"><i class="icon-twitter"></i></a>
                                    <a href="https://www.instagram.com/mohd_sharhan_/#" class="social-icon" title="Instagram" target="_blank"><i class="icon-instagram"></i></a>
                                    <a href="https://www.instagram.com/mohd_sharhan_/#" class="social-icon" title="Youtube" target="_blank"><i class="icon-youtube"></i></a>
                                    <a href="https://www.instagram.com/mohd_sharhan_/#" class="social-icon" title="Pinterest" target="_blank"><i class="icon-pinterest"></i></a>
                                </div><!-- End .soial-icons -->
                            </div><!-- End .widget about-widget -->
                        </div><!-- End .col-sm-6 col-lg-3 -->
    
                        <div class="col-sm-6 col-lg-3">
                            <div class="widget">
                                <h4 class="widget-title">Useful Links</h4><!-- End .widget-title -->
    
                                <ul class="widget-list">
                                    <li><a href="">About Molla</a></li>
                                    <li><a href="#">How to shop on Molla</a></li>
                                    <li><a href="#">FAQ</a></li>
                                    <li><a href="">Contact us</a></li>
                                    <li><a href="">Log in</a></li>
                                </ul><!-- End .widget-list -->
                            </div><!-- End .widget -->
                        </div><!-- End .col-sm-6 col-lg-3 -->
    
                        <div class="col-sm-6 col-lg-3">
                            <div class="widget">
                                <h4 class="widget-title">Customer Service</h4><!-- End .widget-title -->
    
                                <ul class="widget-list">
                                    <li><a href="#">Payment Methods</a></li>
                                    <li><a href="#">Money-back guarantee!</a></li>
                                    <li><a href="#">Returns</a></li>
                                    <li><a href="#">Shipping</a></li>
                                    <li><a href="#">Terms and conditions</a></li>
                                    <li><a href="#">Privacy Policy</a></li>
                                </ul><!-- End .widget-list -->
                            </div><!-- End .widget -->
                        </div>
    
                        <div class="col-sm-6 col-lg-3">
                            <div class="widget">
                                <h4 class="widget-title">My Account</h4><!-- End .widget-title -->
    
                                <ul class="widget-list">
                                    <li><a href="/sin">Sign In</a></li>
                                    <li><a href="/viewcart">View Cart</a></li>
                                    <li><a href="/wishlist">My Wishlist</a></li>
                                    <li><a href="/getorder">Track My Order</a></li>
                                    
                                </ul><!-- End .widget-list -->
                            </div><!-- End .widget -->
                        </div><!-- End .col-sm-6 col-lg-3 -->
                    </div><!-- End .row -->
                </div><!-- End .container -->
            </div><!-- End .footer-middle -->
    
            <div class="footer-bottom">
                <div class="container">
                    <p class="footer-copyright">Copyright © 2019 ELECTRO MANIA Store. All Rights Reserved.</p><!-- End .footer-copyright -->
                    <figure class="footer-payments">
                        <img src="assets/images/payments.png" alt="Payment methods" width="272" height="20">
                    </figure><!-- End .footer-payments -->
                </div><!-- End .container -->
            </div><!-- End .footer-bottom -->
        </footer><!-- End .footer -->
    </div><!-- End .page-wrapper -->

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10.14.0/dist/sweetalert2.all.min.js"></script>







    <%- include('footer.ejs') %>